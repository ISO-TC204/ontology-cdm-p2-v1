    <!-- ssn: -->

    <owl:NamedIndividual rdf:about="ssn:">
        <rdf:type rdf:resource="voaf:Vocabulary"/>
    </owl:NamedIndividual>
    <rdf:Description>
        <rdf:type rdf:resource="foaf:Agent"/>
        <foaf:name xml:lang="en">W3C/OGC Spatial Data on the Web Working Group</foaf:name>
    </rdf:Description>
    <rdf:Description>
        <rdf:type rdf:resource="foaf:Agent"/>
        <foaf:name xml:lang="en">W3C/OGC Spatial Data on the Web Working Group</foaf:name>
    </rdf:Description>
    <rdf:Description>
        <rdf:type rdf:resource="foaf:Agent"/>
        <foaf:name xml:lang="en">W3C/OGC Spatial Data on the Web Working Group</foaf:name>
    </rdf:Description>
    <rdf:Description>
        <rdf:type rdf:resource="foaf:Agent"/>
        <foaf:name xml:lang="en">W3C/OGC Spatial Data on the Web Working Group</foaf:name>
    </rdf:Description>
    <rdf:Description>
        <rdf:type rdf:resource="foaf:Agent"/>
        <foaf:name xml:lang="en">W3C/OGC Spatial Data on the Web Working Group</foaf:name>
    </rdf:Description>
    <rdf:Description>
        <rdf:type rdf:resource="foaf:Agent"/>
        <foaf:name xml:lang="en">W3C/OGC Spatial Data on the Web Working Group</foaf:name>
