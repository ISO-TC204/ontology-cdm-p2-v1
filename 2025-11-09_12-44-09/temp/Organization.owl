    <!-- schema:PostalAddress -->

    <owl:Class rdf:about="schema:PostalAddress"/>
    


    <!-- cityUnits:MonetaryValue -->

    <owl:Class rdf:about="cityUnits:MonetaryValue"/>
    


    <!-- city:JurisdictionalArea -->

    <owl:Class rdf:about="city:JurisdictionalArea"/>
    


    <!-- cityService:Program -->

    <owl:Class rdf:about="cityService:Program"/>
    


    <!-- cityUnits:MonetaryValue -->

    <owl:Class rdf:about="cityUnits:MonetaryValue"/>
    


    <!-- contact:/Address -->

    <owl:Class rdf:about="contact:/Address"/>
    


    <!-- contact:/PhoneNumber -->

    <owl:Class rdf:about="contact:/PhoneNumber"/>
    


    <!-- cdmOrg:BusinessEstablishment -->

    <owl:Class rdf:about="cdmOrg:BusinessEstablishment">
        <rdfs:subClassOf rdf:resource="cdmOrg:CityOrgOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="contact:hasAddress"/>
                <owl:allValuesFrom rdf:resource="contact:/Address"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="spatialLoc:hasLocation"/>
                <owl:maxQualifiedCardinality rdf:datatype="xsd:nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="spatialLoc:Location"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>Business Establishment: A Business establishment is a physical location where a Firm conducts business.
A Business Establishment has a Location and may have an address.</dc:description>
    </owl:Class>
    


    <!-- cdmOrg:BusinessId -->

    <owl:Class rdf:about="cdmOrg:BusinessId"/>
    


    <!-- cdmOrg:CityOrgOntologyThing -->

    <owl:Class rdf:about="cdmOrg:CityOrgOntologyThing">
        <dc:description>Added for organizational purposes, to identify classes defined in the iCity-Organization ontology.</dc:description>
    </owl:Class>
    


    <!-- cdmOrg:Compensation -->

    <owl:Class rdf:about="cdmOrg:Compensation">
        <rdfs:subClassOf rdf:resource="cdmOrg:CityOrgOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="cdmOrg:hasPay"/>
                <owl:maxQualifiedCardinality rdf:datatype="xsd:nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="cityUnits:MonetaryValue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- cdmOrg:Employment -->

    <owl:Class rdf:about="cdmOrg:Employment">
        <rdfs:subClassOf rdf:resource="cdmOrg:CityOrgOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="cdmOrg:employedBy"/>
                <owl:someValuesFrom rdf:resource="cdmOrg:Organization"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="cdmOrg:hasCompensation"/>
                <owl:someValuesFrom rdf:resource="cdmOrg:Compensation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="cdmOrg:employedAs"/>
                <owl:allValuesFrom rdf:resource="cdmOrg:Occupation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="cdmOrg:hasEmploymentStatus"/>
                <owl:allValuesFrom rdf:resource="cdmOrg:EmploymentStatus"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- cdmOrg:EmploymentStatus -->

    <owl:Class rdf:about="cdmOrg:EmploymentStatus">
        <rdfs:subClassOf rdf:resource="cdmOrg:CityOrgOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="code:hasCode"/>
                <owl:allValuesFrom rdf:resource="code:Code"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- cdmOrg:ForProfitOrganization -->

    <owl:Class rdf:about="cdmOrg:ForProfitOrganization">
        <rdfs:subClassOf rdf:resource="cdmOrg:Organization"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="cdmOrg:hasEstablishment"/>
                <owl:allValuesFrom rdf:resource="cdmOrg:BusinessEstablishment"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="cdmOrg:hasIndustryType"/>
                <owl:allValuesFrom rdf:resource="cdmOrg:IndustryType"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- cdmOrg:Goal -->

    <owl:Class rdf:about="cdmOrg:Goal">
        <rdfs:subClassOf rdf:resource="activity:State"/>
        <rdfs:subClassOf rdf:resource="cdmOrg:CityOrgOntologyThing"/>
        <dc:description>A goal represents some state or complex states, and allows for the representation of various groups&apos; responsibilities.</dc:description>
    </owl:Class>
    


    <!-- cdmOrg:GovernmentOrganization -->

    <owl:Class rdf:about="cdmOrg:GovernmentOrganization">
        <rdfs:subClassOf rdf:resource="cdmOrg:Organization"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="cdmOrg:hasProgram"/>
                <owl:allValuesFrom rdf:resource="cityService:Program"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="cdmOrg:jurisdiction"/>
                <owl:allValuesFrom rdf:resource="city:JurisdictionalArea"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- cdmOrg:IndustryType -->

    <owl:Class rdf:about="cdmOrg:IndustryType">
        <rdfs:subClassOf rdf:resource="cdmOrg:CityOrgOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="code:hasCode"/>
                <owl:allValuesFrom rdf:resource="code:Code"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- cdmOrg:NonProfitOrganization -->

    <owl:Class rdf:about="cdmOrg:NonProfitOrganization">
        <rdfs:subClassOf rdf:resource="cdmOrg:Organization"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="cdmOrg:hasProgram"/>
                <owl:allValuesFrom rdf:resource="cityService:Program"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- cdmOrg:Occupation -->

    <owl:Class rdf:about="cdmOrg:Occupation">
        <rdfs:subClassOf rdf:resource="cdmOrg:CityOrgOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="code:hasCode"/>
                <owl:allValuesFrom rdf:resource="code:Code"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>Occupation: An Occupation is performed by some Person.
An Occupation has a type (e.g. sales, skilled trades)</dc:description>
    </owl:Class>
    


    <!-- cdmOrg:Operation -->

    <owl:Class rdf:about="cdmOrg:Operation">
        <rdfs:subClassOf rdf:resource="recurringEvent:RecurringEvent"/>
        <rdfs:subClassOf rdf:resource="cdmOrg:CityOrgOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="cdmOrg:closingTime"/>
                <owl:maxQualifiedCardinality rdf:datatype="xsd:nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="xsd:time"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="cdmOrg:openingTime"/>
                <owl:maxQualifiedCardinality rdf:datatype="xsd:nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="xsd:time"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- cdmOrg:Organization -->

    <owl:Class rdf:about="cdmOrg:Organization">
        <rdfs:subClassOf rdf:resource="cdmOrgStruct:Organization"/>
        <rdfs:subClassOf rdf:resource="cdmOrg:CityOrgOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="spatialLoc:hasLocation"/>
                <owl:allValuesFrom rdf:resource="spatialLoc:Location"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="contact:/hasTelephone"/>
                <owl:allValuesFrom rdf:resource="contact:/PhoneNumber"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="cdmOrg:hasGoal"/>
                <owl:allValuesFrom rdf:resource="cdmOrg:Goal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="cdmOrg:operatingHours"/>
                <owl:allValuesFrom rdf:resource="cdmOrg:Operation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="cdmOrg:orgAddress"/>
                <owl:allValuesFrom rdf:resource="contact:/Address"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- cdmOrg:OrganizationAgent -->

    <owl:Class rdf:about="cdmOrg:OrganizationAgent">
        <rdfs:subClassOf rdf:resource="agent:Agent"/>
        <rdfs:subClassOf rdf:resource="cdmOrg:CityOrgOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="org:memberOf"/>
                <owl:allValuesFrom rdf:resource="cdmOrg:Organization"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="cdmOrg:hasEmployment"/>
                <owl:allValuesFrom rdf:resource="cdmOrg:Employment"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="cdmOrg:hasGoal"/>
                <owl:allValuesFrom rdf:resource="cdmOrg:Goal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="cdmOrg:playsRole"/>
                <owl:allValuesFrom rdf:resource="cdmOrg:Role"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>Organization Agent: Members of an organization.
Organization Agents have goals, authority, and may be members of some team.
An Organization Agent plays a Role within the Organization.</dc:description>
    </owl:Class>
    


    <!-- cdmOrg:Role -->

    <owl:Class rdf:about="cdmOrg:Role">
        <rdfs:subClassOf rdf:resource="org:Role"/>
        <rdfs:subClassOf rdf:resource="cdmOrg:CityOrgOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="cdmOrg:hasGoal"/>
                <owl:allValuesFrom rdf:resource="cdmOrg:Goal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="cdmOrg:hasProcess"/>
                <owl:allValuesFrom rdf:resource="activity:Activity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="cdmOrg:hasResource"/>
                <owl:allValuesFrom rdf:resource="resource:Resource"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>Role: A Role has a single (possibly complex) Goal.
A Role has some authority, requires some skill, and may also have some associated processes.</dc:description>
    </owl:Class>
    


    <!-- cdmOrg:Salary -->

    <owl:Class rdf:about="cdmOrg:Salary">
        <rdfs:subClassOf rdf:resource="cdmOrg:Compensation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="cdmOrg:annualPay"/>
                <owl:maxQualifiedCardinality rdf:datatype="xsd:nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="cityUnits:MonetaryValue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- cdmOrg:Wage -->

    <owl:Class rdf:about="cdmOrg:Wage">
        <rdfs:subClassOf rdf:resource="cdmOrg:Compensation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="cdmOrg:overtimePay"/>
                <owl:allValuesFrom rdf:resource="cityUnits:MonetaryValue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
