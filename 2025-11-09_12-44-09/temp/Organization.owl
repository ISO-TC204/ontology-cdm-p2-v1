    <owl:ObjectProperty rdf:about="cdmOrg:jurisdiction">
        <rdfs:subPropertyOf rdf:resource="cdmOrg:CityOrgOntologyProperty"/>
    </owl:ObjectProperty>
    


    <!-- cdmOrg:operatingHours -->

    <owl:ObjectProperty rdf:about="cdmOrg:operatingHours">
        <rdfs:subPropertyOf rdf:resource="cdmOrg:CityOrgOntologyProperty"/>
    </owl:ObjectProperty>
    


    <!-- cdmOrg:orgAddress -->

    <owl:ObjectProperty rdf:about="cdmOrg:orgAddress">
        <rdfs:subPropertyOf rdf:resource="cdmOrg:CityOrgOntologyProperty"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="org:hasSite"/>
            <rdf:Description rdf:about="org:siteAddress"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- cdmOrg:overtimePay -->

    <owl:ObjectProperty rdf:about="cdmOrg:overtimePay">
        <rdfs:subPropertyOf rdf:resource="cdmOrg:hasPay"/>
    </owl:ObjectProperty>
    


    <!-- cdmOrg:playsRole -->

    <owl:ObjectProperty rdf:about="cdmOrg:playsRole">
        <rdfs:subPropertyOf rdf:resource="cdmOrg:CityOrgOntologyProperty"/>
    </owl:ObjectProperty>
    


