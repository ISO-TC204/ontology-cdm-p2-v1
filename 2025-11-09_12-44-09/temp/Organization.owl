    <!-- cdmOrg:CityOrganizationDataProperty -->

    <owl:DatatypeProperty rdf:about="cdmOrg:CityOrganizationDataProperty"/>
    


    <!-- cdmOrg:closingTime -->

    <owl:DatatypeProperty rdf:about="cdmOrg:closingTime">
        <rdfs:subPropertyOf rdf:resource="recurringEvent:endTime"/>
