     xmlns:city="https://standards.iso.org/iso-iec/5087/-2/ed-1/en/ontology/City#"
     xmlns:cityService="https://standards.iso.org/iso-iec/5087/-2/ed-1/en/ontology/CityService#"
     xmlns:code="https://standards.iso.org/iso-iec/5087/-2/ed-1/en/ontology/Code#"
     xmlns:contact="https://standards.iso.org/iso-iec/5087/-2/ed-1/en/ontology/Contact#">
    <owl:Ontology rdf:about="https://standards.iso.org/iso-iec/5087/-2/ed-1/en/ontology/Organization#">
        <owl:imports rdf:resource="http://ontology.eil.utoronto.ca/5087/1/Activity.owl"/>
        <owl:imports rdf:resource="http://ontology.eil.utoronto.ca/5087/1/OrganizationStructure.owl"/>
        <owl:imports rdf:resource="http://ontology.eil.utoronto.ca/5087/1/RecurringEvent.owl"/>
        <owl:imports rdf:resource="http://ontology.eil.utoronto.ca/5087/1/Resource.owl"/>
        <owl:imports rdf:resource="http://ontology.eil.utoronto.ca/5087/1/SpatialLoc.owl"/>
        <owl:imports rdf:resource="http://ontology.eil.utoronto.ca/5087/2/City.owl"/>
        <owl:imports rdf:resource="http://ontology.eil.utoronto.ca/5087/2/Code.owl"/>
        <owl:imports rdf:resource="http://xontology.eil.utoronto.ca/5087/2/Contact.owl"/>
        <dc:title>City Organization Ontology</dc:title>
        <vann:preferredNamespacePrefix>org</vann:preferredNamespacePrefix>
        <adms:relatedDocumentation>Under development. Refer to latest version of ISO 5087-2.</adms:relatedDocumentation>
    </owl:Ontology>
    


