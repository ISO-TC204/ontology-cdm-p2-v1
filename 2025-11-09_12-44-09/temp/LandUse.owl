    <!-- :LandUseOntologyProperty -->

    <owl:ObjectProperty rdf:about=":LandUseOntologyProperty">
        <dc:description>Added for organizational purposes, to identify all properties defined in the Land Use ontology.</dc:description>
    </owl:ObjectProperty>
    


    <!-- :associatedArea -->

    <owl:ObjectProperty rdf:about=":associatedArea">
        <rdfs:subPropertyOf rdf:resource=":LandUseOntologyProperty"/>
    </owl:ObjectProperty>
    


    <!-- :hasCardinality -->

    <owl:ObjectProperty rdf:about=":hasCardinality">
        <rdfs:subPropertyOf rdf:resource=":LandUseOntologyProperty"/>
        <owl:inverseOf rdf:resource="i72:cardinality_of"/>
    </owl:ObjectProperty>
    


    <!-- :hasEmployedPopulation -->

    <owl:ObjectProperty rdf:about=":hasEmployedPopulation">
        <rdfs:subPropertyOf rdf:resource=":hasResidentPopulation"/>
        <rdfs:range rdf:resource=":EmployedPopulation"/>
        <rdfs:comment>A Parcel may have a number of employed residents that is subject to change over time.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- :hasLandArea -->

    <owl:ObjectProperty rdf:about=":hasLandArea">
        <rdfs:subPropertyOf rdf:resource=":LandUseOntologyProperty"/>
    </owl:ObjectProperty>
    


    <!-- :hasLandUse -->

    <owl:ObjectProperty rdf:about=":hasLandUse">
        <rdfs:subPropertyOf rdf:resource=":LandUseOntologyProperty"/>
    </owl:ObjectProperty>
    


    <!-- :hasPopulation -->

    <owl:ObjectProperty rdf:about=":hasPopulation">
        <rdfs:subPropertyOf rdf:resource=":LandUseOntologyProperty"/>
        <rdfs:comment>A Parcel may have some population that is subject to change over time.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- :hasResidentPopulation -->

    <owl:ObjectProperty rdf:about=":hasResidentPopulation">
        <rdfs:subPropertyOf rdf:resource=":hasPopulation"/>
        <rdfs:range rdf:resource=":ResidentPopulation"/>
        <rdfs:comment>A parcel may have some population of residents at a given point in time.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- :parcelHasLocation -->

    <owl:ObjectProperty rdf:about=":parcelHasLocation">
        <rdfs:subPropertyOf rdf:resource="spatialLoc:hasLocation"/>
        <rdfs:subPropertyOf rdf:resource=":LandUseOntologyProperty"/>
        <cityUnits:isInvariant rdf:datatype="xsd:boolean">true</cityUnits:isInvariant>
    </owl:ObjectProperty>
    


    <!-- i72:cardinality_of -->

    <owl:ObjectProperty rdf:about="i72:cardinality_of"/>
    


