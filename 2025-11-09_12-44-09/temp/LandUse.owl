    <!-- cityUnits:Area -->

    <owl:Class rdf:about="cityUnits:Area"/>
    


    <!-- :EmployedPopulation -->

    <owl:Class rdf:about=":EmployedPopulation">
        <rdfs:subClassOf rdf:resource=":ResidentPopulation"/>
        <rdfs:comment>The population of employed residents in a given area (Parcel)</rdfs:comment>
    </owl:Class>
    


    <!-- :LandArea -->

    <owl:Class rdf:about=":LandArea">
        <rdfs:subClassOf rdf:resource="change:Manifestation"/>
        <rdfs:subClassOf rdf:resource=":LandUseOntologyThing"/>
        <rdfs:subClassOf rdf:resource=":Parcel"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource=":parcelHasLocation"/>
                <owl:someValuesFrom rdf:resource="i72:Feature"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource=":associatedArea"/>
                <owl:allValuesFrom rdf:resource="cityUnits:Area"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource=":hasLandUse"/>
                <owl:allValuesFrom rdf:resource=":LandUseClassification"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource=":hasPopulation"/>
                <owl:allValuesFrom rdf:resource="i72:Population"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="change:existsAt"/>
                <owl:qualifiedCardinality rdf:datatype="xsd:nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="time:TemporalEntity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- :LandUseClassification -->

    <owl:Class rdf:about=":LandUseClassification">
        <rdfs:subClassOf rdf:resource=":LandUseOntologyThing"/>
        <rdfs:comment>Land Use Classifications provide a means of describing the land cover/use in a standard way. Various classification systems are used to identify types of land use. Currently, we include LBCS, CLUMP, and AAFC.</rdfs:comment>
    </owl:Class>
    


    <!-- :LandUseOntologyThing -->

    <owl:Class rdf:about=":LandUseOntologyThing">
        <rdfs:comment>Added for organizational purposes, to identify all classes defined in the Land Use ontology.</rdfs:comment>
    </owl:Class>
    


    <!-- :Parcel -->

    <owl:Class rdf:about=":Parcel">
        <rdfs:subClassOf rdf:resource=":LandUseOntologyThing"/>
        <dc:description>A Parcel is a way of defining some area in an urban system.
A Parcel has a Location.
A Parcel may be classified as having some type of Land Use.
There may be other types (subclasses) of Parcel, defined in more precise or different ways, such as a Zone.</dc:description>
    </owl:Class>
    


    <!-- :ResidentPopulation -->

    <owl:Class rdf:about=":ResidentPopulation">
        <rdfs:subClassOf rdf:resource=":LandUseOntologyThing"/>
        <rdfs:subClassOf rdf:resource="i72:Population"/>
        <rdfs:comment>The population that resides within a given area (Parcel)</rdfs:comment>
    </owl:Class>
    


    <!-- :TrafficZone -->

    <owl:Class rdf:about=":TrafficZone">
        <rdfs:subClassOf rdf:resource=":LandArea"/>
    </owl:Class>
    


    <!-- i72:Feature -->

    <owl:Class rdf:about="i72:Feature"/>
    


    <!-- i72:Population -->

    <owl:Class rdf:about="i72:Population"/>
</rdf:RDF>



